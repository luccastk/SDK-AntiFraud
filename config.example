# SDK AntiFraud - Configurações de Desenvolvimento

# Backend Kotlin
KOTLIN_API_PORT=8080
KOTLIN_API_HOST=localhost
KOTLIN_API_URL=http://localhost:8080

# Frontend Node.js
NODE_APP_PORT=3000
NODE_APP_HOST=localhost

# Configurações de Desenvolvimento
NODE_ENV=development
LOG_LEVEL=debug

# Configurações de Antifraude
DEFAULT_RISK_THRESHOLD=40
MAX_RISK_SCORE=100
IP_VERIFICATION_ENABLED=true
FINGERPRINT_VERIFICATION_ENABLED=true

# Timeouts (em milissegundos)
API_TIMEOUT=10000
FINGERPRINT_COLLECTION_TIMEOUT=5000

# Configurações de Log
LOG_BACKEND=true
LOG_FRONTEND=true
LOG_VERIFICATION_RESULTS=true

# Configurações de Segurança
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
CORS_ENABLED=true

